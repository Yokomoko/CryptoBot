<Application x:Class="CryptoBot.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CryptoBot"
             xmlns:converters="clr-namespace:CryptoBot.Resources.Converters"
             xmlns:tb="http://www.hardcodet.net/taskbar"
             Startup="Application_Startup"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <tb:TaskbarIcon x:Key="NotificationIcon" IconSource="Bitcoin.ico" ToolTipText="Open CryptoBot"></tb:TaskbarIcon>
        </ResourceDictionary>
    </Application.Resources>
</Application>
